<?xml version="1.0" encoding="utf-8"?>
<org.lasque.tusdkvideodemo.views.TuSeekBarPressure
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <View
        android:id="@+id/lsq_seekBottomView"
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:layout_centerVertical="true"
        android:background="@drawable/tusdk_view_widget_seekbar_bottom_bg" />

    <View
        android:id="@+id/lsq_seekTopView"
        android:layout_width="0dp"
        android:layout_height="4dp"
        android:layout_centerVertical="true"
        android:background="@drawable/tusdk_view_widget_seekbar_top_bg" />

    <View
        android:id="@+id/lsq_seekDrag"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_centerVertical="true"
        android:background="@drawable/tusdk_view_widget_seekbar_drag" />

    <View
        android:id="@+id/lsq_seekSecond"
        android:layout_width="6dp"
        android:layout_height="6dp"
        android:layout_centerVertical="true"
        android:background="@drawable/tusdk_view_widget_seekbar_drag" />

</org.lasque.tusdkvideodemo.views.TuSeekBarPressure>